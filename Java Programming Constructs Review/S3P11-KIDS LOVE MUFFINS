import java.util.Scanner;
class Main
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int flag=0;
        for(int i=0;i<a;i++)
        {
            int c=sc.nextInt();
            flag=flag+(c/b);
        }
        System.out.println(flag);

    }
}
