import java.util.Scanner;
class Main
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner (System.in);
        int n=sc.nextInt();
       if(n<=14)
       {
           System.out.println("Child Ticket");
       }
       else
       {
           System.out.println("Adult Ticket");
       }
       
    }
}
