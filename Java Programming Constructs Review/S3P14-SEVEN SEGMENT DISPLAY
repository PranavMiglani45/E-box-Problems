import java.util.Scanner;

public class Main {
    public static void main(String args[]) {
        Scanner scan = new Scanner(System.in);
        int num =  scan.nextInt();
        int r=0;
        if(num==0)
        {
            r=1;
            System.out.println(r);
        }
        else
        {
            r=0;
        while(num!=0)
        {
            r++;
            num=num/10;
        }
        System.out.println(r);
        }
       
    }
}
